		<PackageIcon>icon.png</PackageIcon>
		<AssemblyName>Ardalis.SmartEnum.JsonNet</AssemblyName>
		<RootNamespace>Ardalis.SmartEnum.JsonNet</RootNamespace>
	</PropertyGroup>
	<PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
